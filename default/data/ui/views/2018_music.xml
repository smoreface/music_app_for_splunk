<dashboard>
  <label>2018 Music</label>
  <row>
    <panel>
      <title>Top 10 Artists of 2018</title>
      <chart>
        <search>
          <query>`lastfm` | dedup uts | timechart count(artist) by artist useother=f usenull=f| sort -count(artist) limit=10</query>
          <earliest>1514793600</earliest>
          <latest>1546329600</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
