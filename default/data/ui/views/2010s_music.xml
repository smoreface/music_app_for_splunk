    <panel>
      <title>Bands discovered in the 2010s</title>
      <single>
        <search>
          <query>`lastfm_earliest` | search earliestyear IN (2010,2011,2012,2013,2014,2015,2016,2017,2018,2019) | stats count</query>
          <earliest>0</earliest>
          <latest></latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </single>
