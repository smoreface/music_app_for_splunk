<dashboard version="1.1">
  <label>Concert Statistics</label>
  <row>
    <panel>
      <title>Number of Concerts Skipped</title>
      <single>
        <search>
          <query>|inputlookup ticketpurchase.csv | table purchase_date,concert_date,artist,status | fillnull status | search status!=cancelled status!=postponed | search NOT [|inputlookup concerts.csv | makemv artist delim="," | mvexpand artist| table artist] | eval current_time=now() | convert ctime(tnow) | eval concertdate=round(strptime(concert_date,"%B %d %Y")) | where concertdate&lt;current_time | stats count</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="unitPosition">before</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
    <panel>
      <title>Money Spent on Concerts Skipped</title>
      <single>
        <search>
          <query>|inputlookup ticketpurchase.csv | table concert_date,artist,cost,quantity | fillnull status | search status!=cancelled status!=postponed | search NOT [|inputlookup concerts.csv | makemv artist delim="," | mvexpand artist| table artist]| eval current_time=now() | convert ctime(tnow) | eval concertdate=round(strptime(concert_date,"%B %d %Y")) | where concertdate&lt;current_time | stats sum(cost)</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="unit">$</option>
        <option name="unitPosition">before</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
    <panel>
      <title>Average Days Between Ticket Purchase and Concert - Concerts Skipped</title>
      <single>
        <search>
          <query>|inputlookup ticketpurchase.csv | table purchase_date,concert_date,artist | fillnull status | search status!=cancelled status!=postponed | search NOT [|inputlookup concerts.csv | makemv artist delim="," | mvexpand artist| table artist]| eval current_time=now() | convert ctime(tnow) | eval purchased=round(strptime(purchase_date,"%B %d %Y")) | eval concertdate=round(strptime(concert_date,"%B %d %Y")) | where concertdate&lt;current_time | eval lag=(concertdate-purchased) | eval lag=round(lag/86400) | stats avg(lag) as average | eval average_lag=round(average) | table average_lag</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="unit">days</option>
        <option name="unitPosition">after</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
    <panel>
      <title>Average Cost of Concerts Skipped</title>
      <single>
        <search>
          <query>|inputlookup ticketpurchase.csv | table concert_date,artist,cost,quantity | fillnull status | search status!=cancelled status!=postponed | search NOT [|inputlookup concerts.csv | makemv artist delim="," | mvexpand artist| table artist]| eval current_time=now() | convert ctime(tnow) | eval concertdate=round(strptime(concert_date,"%B %d %Y")) | where concertdate&lt;current_time | stats sum(cost) as total_cost,sum(quantity) as total_quantity | eval average_cost=(total_cost/total_quantity) | table average_cost</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="unit">$</option>
        <option name="unitPosition">before</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
  </row>
